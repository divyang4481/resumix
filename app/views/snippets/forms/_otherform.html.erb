<%= f.simple_fields_for :properties, OpenStruct.new(f.object.properties.to_hash) do |ff| %>
  <%= ff.input :title, required: false %>
  <%= ff.input :custom, required: false %>
  <%= ff.input :custom2, required: false, input_html: { rows: 4 } %>
  <%= ff.input :start_date, as: :string, required: false, input_html: {class: "datepicker"} %>
  <%= ff.input :end_date, as: :string, required: false, input_html: {class: "datepicker"} %>
<% end %>
